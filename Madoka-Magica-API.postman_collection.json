{
  "info": {
    "name": "Madoka Magica",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    { "key": "baseUrl", "value": "http://localhost:8080" },
    { "key": "magicalGirlId", "value": "" },
    { "key": "witchId", "value": "" },
    { "key": "familiarId", "value": "" }
  ],
  "item": [
    { "name": "GET /", "request": { "method": "GET", "url": "{{baseUrl}}/" } },
    { "name": "GET /index", "request": { "method": "GET", "url": "{{baseUrl}}/index" } },
    {
      "name": "POST /magicalgirls",
      "request": {
        "method": "POST",
        "header": [{ "key": "Content-Type", "value": "application/json" }],
        "body": { "mode": "raw", "raw": "{\"name\":\"Madoka Kaname\",\"soulGemColor\":\"Pink\",\"weapon\":\"Bow\",\"powerLevel\":95}" },
        "url": "{{baseUrl}}/magicalgirls",
        "event": [{ "listen": "test", "script": { "exec": "if (pm.response.code === 201) { var j = pm.response.json(); if (j && j._id) pm.collectionVariables.set('magicalGirlId', j._id); }", "type": "text/javascript" } }]
      }
    },
    {
      "name": "POST /witches",
      "request": {
        "method": "POST",
        "header": [{ "key": "Content-Type", "value": "application/json" }],
        "body": { "mode": "raw", "raw": "{\"name\":\"Gertrud\",\"barrierType\":\"Labyrinth\",\"dangerLevel\":6,\"magicalGirl\":\"{{magicalGirlId}}\"}" },
        "url": "{{baseUrl}}/witches",
        "event": [{ "listen": "test", "script": { "exec": "if (pm.response.code === 201) { var j = pm.response.json(); if (j && j._id) pm.collectionVariables.set('witchId', j._id); }", "type": "text/javascript" } }]
      }
    },
    {
      "name": "POST /familiars",
      "request": {
        "method": "POST",
        "header": [{ "key": "Content-Type", "value": "application/json" }],
        "body": { "mode": "raw", "raw": "{\"name\":\"Anthony\",\"witch\":\"{{witchId}}\",\"type\":\"Minion\",\"strength\":15}" },
        "url": "{{baseUrl}}/familiars",
        "event": [{ "listen": "test", "script": { "exec": "if (pm.response.code === 201) { var j = pm.response.json(); if (j && j._id) pm.collectionVariables.set('familiarId', j._id); }", "type": "text/javascript" } }]
      }
    },
    { "name": "GET /magicalgirls", "request": { "method": "GET", "url": "{{baseUrl}}/magicalgirls" } },
    { "name": "GET /magicalgirls/:id", "request": { "method": "GET", "url": "{{baseUrl}}/magicalgirls/{{magicalGirlId}}" } },
    { "name": "GET /witches", "request": { "method": "GET", "url": "{{baseUrl}}/witches" } },
    { "name": "GET /witches/:id", "request": { "method": "GET", "url": "{{baseUrl}}/witches/{{witchId}}" } },
    { "name": "GET /familiars", "request": { "method": "GET", "url": "{{baseUrl}}/familiars" } },
    { "name": "GET /familiars/:id", "request": { "method": "GET", "url": "{{baseUrl}}/familiars/{{familiarId}}" } },
    { "name": "GET /list", "request": { "method": "GET", "url": "{{baseUrl}}/list" } },
    { "name": "GET /list-witches", "request": { "method": "GET", "url": "{{baseUrl}}/list-witches" } },
    { "name": "GET /list-familiars", "request": { "method": "GET", "url": "{{baseUrl}}/list-familiars" } },
    { "name": "GET /view/:id", "request": { "method": "GET", "url": "{{baseUrl}}/view/{{magicalGirlId}}" } },
    { "name": "GET /witch/:id", "request": { "method": "GET", "url": "{{baseUrl}}/witch/{{witchId}}" } },
    { "name": "GET /familiar/:id", "request": { "method": "GET", "url": "{{baseUrl}}/familiar/{{familiarId}}" } },
    { "name": "GET /new", "request": { "method": "GET", "url": "{{baseUrl}}/new" } },
    { "name": "GET /new-witch", "request": { "method": "GET", "url": "{{baseUrl}}/new-witch" } },
    { "name": "GET /new-familiar", "request": { "method": "GET", "url": "{{baseUrl}}/new-familiar" } },
    { "name": "GET /edit/:id", "request": { "method": "GET", "url": "{{baseUrl}}/edit/{{magicalGirlId}}" } },
    { "name": "GET /edit-witch/:id", "request": { "method": "GET", "url": "{{baseUrl}}/edit-witch/{{witchId}}" } },
    { "name": "GET /edit-familiar/:id", "request": { "method": "GET", "url": "{{baseUrl}}/edit-familiar/{{familiarId}}" } },
    {
      "name": "PUT /magicalgirls/:id",
      "request": {
        "method": "PUT",
        "header": [{ "key": "Content-Type", "value": "application/json" }],
        "body": { "mode": "raw", "raw": "{\"name\":\"Madoka Kaname\",\"soulGemColor\":\"Pink\",\"weapon\":\"Bow\",\"powerLevel\":100}" },
        "url": "{{baseUrl}}/magicalgirls/{{magicalGirlId}}"
      }
    },
    {
      "name": "PUT /witches/:id",
      "request": {
        "method": "PUT",
        "header": [{ "key": "Content-Type", "value": "application/json" }],
        "body": { "mode": "raw", "raw": "{\"name\":\"Gertrud\",\"barrierType\":\"Labyrinth\",\"dangerLevel\":7,\"defeated\":true}" },
        "url": "{{baseUrl}}/witches/{{witchId}}"
      }
    },
    {
      "name": "PUT /familiars/:id",
      "request": {
        "method": "PUT",
        "header": [{ "key": "Content-Type", "value": "application/json" }],
        "body": { "mode": "raw", "raw": "{\"name\":\"Anthony\",\"witch\":\"{{witchId}}\",\"type\":\"Elite\",\"strength\":30}" },
        "url": "{{baseUrl}}/familiars/{{familiarId}}"
      }
    },
    {
      "name": "POST /edit/:id",
      "request": {
        "method": "POST",
        "header": [{ "key": "Content-Type", "value": "application/x-www-form-urlencoded" }],
        "body": { "mode": "urlencoded", "urlencoded": [
          { "key": "name", "value": "Madoka Kaname" },
          { "key": "soulGemColor", "value": "Pink" },
          { "key": "weapon", "value": "Bow" },
          { "key": "powerLevel", "value": "100" }
        ]},
        "url": "{{baseUrl}}/edit/{{magicalGirlId}}"
      }
    },
    {
      "name": "POST /edit-witch/:id",
      "request": {
        "method": "POST",
        "header": [{ "key": "Content-Type", "value": "application/x-www-form-urlencoded" }],
        "body": { "mode": "urlencoded", "urlencoded": [
          { "key": "name", "value": "Gertrud" },
          { "key": "barrierType", "value": "Labyrinth" },
          { "key": "dangerLevel", "value": "7" },
          { "key": "defeated", "value": "on" }
        ]},
        "url": "{{baseUrl}}/edit-witch/{{witchId}}"
      }
    },
    {
      "name": "POST /edit-familiar/:id",
      "request": {
        "method": "POST",
        "header": [{ "key": "Content-Type", "value": "application/x-www-form-urlencoded" }],
        "body": { "mode": "urlencoded", "urlencoded": [
          { "key": "name", "value": "Anthony" },
          { "key": "witch", "value": "{{witchId}}" },
          { "key": "type", "value": "Elite" },
          { "key": "strength", "value": "30" }
        ]},
        "url": "{{baseUrl}}/edit-familiar/{{familiarId}}"
      }
    },
    {
      "name": "POST /process",
      "request": {
        "method": "POST",
        "header": [{ "key": "Content-Type", "value": "application/x-www-form-urlencoded" }],
        "body": { "mode": "urlencoded", "urlencoded": [
          { "key": "name", "value": "Sayaka Miki" },
          { "key": "soulGemColor", "value": "Blue" },
          { "key": "weapon", "value": "Sword" },
          { "key": "powerLevel", "value": "80" }
        ]},
        "url": "{{baseUrl}}/process"
      }
    },
    {
      "name": "POST /process-witch",
      "request": {
        "method": "POST",
        "header": [{ "key": "Content-Type", "value": "application/x-www-form-urlencoded" }],
        "body": { "mode": "urlencoded", "urlencoded": [
          { "key": "name", "value": "Charlotte" },
          { "key": "barrierType", "value": "Labyrinth" },
          { "key": "dangerLevel", "value": "6" }
        ]},
        "url": "{{baseUrl}}/process-witch"
      }
    },
    {
      "name": "POST /process-familiar",
      "request": {
        "method": "POST",
        "header": [{ "key": "Content-Type", "value": "application/x-www-form-urlencoded" }],
        "body": { "mode": "urlencoded", "urlencoded": [
          { "key": "name", "value": "Pyotr" },
          { "key": "witch", "value": "{{witchId}}" },
          { "key": "type", "value": "Minion" },
          { "key": "strength", "value": "10" }
        ]},
        "url": "{{baseUrl}}/process-familiar"
      }
    },
    { "name": "POST /view/:id/delete", "request": { "method": "POST", "url": "{{baseUrl}}/view/{{magicalGirlId}}/delete" } },
    { "name": "DELETE /magicalgirls/:id", "request": { "method": "DELETE", "url": "{{baseUrl}}/magicalgirls/{{magicalGirlId}}" } },
    { "name": "POST /witch/:id/delete", "request": { "method": "POST", "url": "{{baseUrl}}/witch/{{witchId}}/delete" } },
    { "name": "DELETE /witches/:id", "request": { "method": "DELETE", "url": "{{baseUrl}}/witches/{{witchId}}" } },
    { "name": "POST /familiar/:id/delete", "request": { "method": "POST", "url": "{{baseUrl}}/familiar/{{familiarId}}/delete" } },
    { "name": "DELETE /familiars/:id", "request": { "method": "DELETE", "url": "{{baseUrl}}/familiars/{{familiarId}}" } }
  ]
}
